<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="ISMNewsPortal.DAL" namespace="ISMNewsPortal.DAL.Models">
  <class name="Admin" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" type="int">
      <generator class="identity" />
    </id>
    <property name="Login"
              type="string"
              length ="128"
              not-null="true"/>
    <property name="Password"
              type="string"
              length ="128"
              not-null="true"/>
    <property name="Salt"
              type="string"
              length ="128"
              not-null="true"/>
    <property name="Roles"
              type="string"/>
    <property name="Email"
              length ="512"
              type="string"/>
  </class>
</hibernate-mapping>