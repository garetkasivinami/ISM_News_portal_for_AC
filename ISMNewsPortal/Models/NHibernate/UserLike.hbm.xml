<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="ISMNewsPortal" namespace="ISMNewsPortal.Models">
  <class name="UserLike" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" type="int">
      <generator class="identity" />
    </id>
    <many-to-one name="NewsPost"
                 class="NewsPost"
                 column="NewsPostId"
                 cascade = "save-update"
                 not-null="true"/>
    <many-to-one name="User"
                 class="Users"
                 column="UserId"
                 cascade = "save-update"
                 not-null="true"/>
  </class>
</hibernate-mapping>